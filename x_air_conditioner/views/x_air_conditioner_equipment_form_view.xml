<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Air Conditioner equiment form : views -->
    <record id="x_air_conditioner_equipment_form_view" model="ir.ui.view">
        <field name="name">Air Conditioner Equipment Form</field>
        <field name="model">maintenance.equipment</field>
        <field name="inherit_id" ref="x_maintenance.x_maintenance_hr_equipment_view_form"/>
        <field name="mode">primary</field>
        <field eval="25" name="priority"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='x_vehicle_id']" position="attributes">
                <attribute name='invisible'>1</attribute>
            </xpath>

        </field>
    </record>

</odoo>
