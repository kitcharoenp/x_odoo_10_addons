<?xml version="1.0" encoding="UTF-8"?>
<odoo>
<record id='x_fleet_vehicle_tree_view' model='ir.ui.view'>
    <field name="name">Fleet vehicle Tree</field>
    <field name="model">fleet.vehicle</field>
    <field name="inherit_id"
        ref="fleet.fleet_vehicle_view_tree"/>
    <field name="arch" type="xml">
        <xpath expr="//field[@name='driver_id']" position="replace">
            <field name="x_driver_id1" />
        </xpath>
    </field>
</record>
</odoo>
