<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="x_fleet_vehicle_claims_form_view" model="ir.ui.view">
        <field name="name">Fleet vehicle claims form</field>
        <field name="model">fleet.vehicle.claims</field>
        <field name="arch" type="xml">
            <form string="Withholding Tax Document">
               <sheet string="Vehicle Claims">
                    <label for="name"/>
                    <h1>
                        <field name="name"/>
                    </h1>
                    <group>
                        <group>
                        </group>
                        <group>
                        </group>
                    </group>
                    <field name='description' placeholder="Internal Note ......."/>
                </sheet>

                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>
</odoo>
